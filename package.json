{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test:react": "react-scripts test --env=jsdom",
    "eject:react": "react-scripts eject",
    "start:electron": "electron .",
    "build:electron": "",
    "start": "concurrently -k \"cross-env BROWSER=none npm run start:react\" \"wait-on tcp:3000 && npm run start:electron\""
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.0.1",
    "react": "19.0.0-rc-3edc000d-20240926",
    "react-dom": "19.0.0-rc-3edc000d-20240926",
    "react-scripts": "^5.0.0",
    "wait-on": "^8.0.2"
  },
  "devDependencies": {
    "ajv": "^8.17.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}